{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      },
      {
        "src": "index.js",
        "use": "@vercel/static-build",
        "config": {
          "includedFiles": ["dist/**/*"]
        }
      }
    ],
    "routes": [
      {
        "src": "/.*",
        "dest": "/index.js"
      }
    ]
  }